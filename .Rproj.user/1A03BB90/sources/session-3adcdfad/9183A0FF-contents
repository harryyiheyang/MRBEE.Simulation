library(MASS)
library(MendelianRandomization)
library(MRMix)
library(MRBEE.IMRP)
library(FDRestimation)
setwd("~/MRBEE-Robust/")
source("mainfunction.R")
CHP=1;UUHP=0;
M=200;n=50000;hx=0.1;overlap=1
filename="MVSimu/CP-200-over1.RData"
N1=c(1:n);N2=N1+round((1-overlap)*n);
N=max(N2)
theta=c(0,0.2,-0.2,0.4)
SigmaB=CScov(4,0.5)
SigmaE=CScov(6,0.5)
TIME=RES11=RES12=RES13=RES14=RES21=RES22=RES23=RES24=matrix(0,500,7)
istar=1;adjust.method="Sidak";var.est="robust";
FDR=T;
source("MVprogram.R")

